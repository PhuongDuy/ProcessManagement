<div class="row">
    <div class="col-lg-8 col-md-7">
        <div class="card" style="padding-bottom:0">
            <div class="header">
                <h2>Members of @Model.Name</h2>
            </div>
            <div class="body" style="padding-top:0">
                <div class="table-responsive">
                    <table class="table table-hover dataTable table-custom m-b-0" id="table-member">
                        <thead>
                            <tr>
                                <th>Member</th>
                                <th>Role</th>
                                <th></th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>
                    <div id="add-member-search">
                        <input type="hidden" style="width:calc(100% - 116px);border: 1px solid #ced4da;padding:5px 10px;" />
                        <button class="btn btn-primary-theme" id="btn-add-member" data-loading-text="<i class='fa fa-circle-o-notch fa-spin'></i> Processing">Add member</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-lg-4 col-md-5">
